    void checkWinner() {
        if ((btn00flag == false) && (btn00.getText().equals(btn01.getText())) && (btn01.getText().equals(btn02.getText()))) {
            
//            btn02.setBorderPainted(true);
            String value = btn00.getText();
            JOptionPane.showMessageDialog(null, "Congratulations, " + value + " wins !!");
        
        } 
        else if((btn10flag == false) && (btn10.getText().equals(btn11.getText())) && (btn11.getText().equals(btn12.getText())))
        {
            {

                String value = btn10.getText();
                JOptionPane.showMessageDialog(null, "Congratulations, " + value + " wins !!");

            }
        }
        else if((btn20flag == false) && (btn20.getText().equals(btn21.getText())) && (btn21.getText().equals(btn22.getText())))
        {
            {

                String value = btn20.getText();
                JOptionPane.showMessageDialog(null, "Congratulations, " + value + " wins !!");

            }
        }
        else if((btn00flag == false) && (btn00.getText().equals(btn10.getText())) && (btn10.getText().equals(btn20.getText())))
        {
            {

                String value = btn00.getText();
                JOptionPane.showMessageDialog(null, "Congratulations, " + value + " wins !!");

            }
        }
        else if((btn01flag == false) && (btn01.getText().equals(btn11.getText())) && (btn11.getText().equals(btn12.getText())))
        {
            {

                String value = btn01.getText();
                JOptionPane.showMessageDialog(null, "Congratulations, " + value + " wins !!");

            }
        }
        else if((btn02flag == false) && (btn02.getText().equals(btn12.getText())) && (btn12.getText().equals(btn22.getText())))
        {
            {

                String value = btn02.getText();
                JOptionPane.showMessageDialog(null, "Congratulations, " + value + " wins !!");

            }
        }
          else if((btn00flag == false) && (btn00.getText().equals(btn11.getText())) && (btn11.getText().equals(btn22.getText())))
        {
            {

                String value = btn00.getText();
                JOptionPane.showMessageDialog(null, "Congratulations, " + value + " wins !!");

            }
        }
        else if((btn02flag == false) && (btn02.getText().equals(btn11.getText())) && (btn11.getText().equals(btn20.getText())))
        {
            {

                String value = btn02.getText();
                JOptionPane.showMessageDialog(null, "Congratulations, " + value + " wins !!");

            }
        }
    }

