@Test
public void testPopulateFxRateDataWithValidInput() {
    // Arrange
    List<Result> fxRateData = Arrays.asList(
        new Result("USD", "EUR", "1.2345"),
        new Result("GBP", "USD", "1.5678")
    );
    String asAtDate = "2022-03-05";
    Long rateSetId = 123L;
    FxRateEntityEntityHelper helper = new FxRateEntityEntityHelper();

    // Act
    List<FxRateEntity> fxRateEntities = helper.populateFxRateData(fxRateData, asAtDate, rateSetId);

    // Assert
    assertEquals(2, fxRateEntities.size());
    assertEquals("USD", fxRateEntities.get(0).getFromCurrency());
    assertEquals("EUR", fxRateEntities.get(0).getToCurrency());
    assertEquals(new BigDecimal("1.2345"), fxRateEntities.get(0).getFxRate());
    assertEquals(LocalDate.parse(asAtDate), fxRateEntities.get(0).getFxRateDate());
    assertEquals(rateSetId, fxRateEntities.get(0).getFxRateSetId());
    assertEquals("GBP", fxRateEntities.get(1).getFromCurrency());
    assertEquals("USD", fxRateEntities.get(1).getToCurrency());
    assertEquals(new BigDecimal("1.5678"), fxRateEntities.get(1).getFxRate());
    assertEquals(LocalDate.parse(asAtDate), fxRateEntities.get(1).getFxRateDate());
    assertEquals(rateSetId, fxRateEntities.get(1).getFxRateSetId());
}



@Test
public void testPopulateFxRateDataWithEmptyInput() {
    // Arrange
    List<Result> fxRateData = Collections.emptyList();
    String asAtDate = "2022-03-05";
    Long rateSetId = 123L;
    FxRateEntityEntityHelper helper = new FxRateEntityEntityHelper();

    // Act
    List<FxRateEntity> fxRateEntities = helper.populateFxRateData(fxRateData, asAtDate, rateSetId);

    // Assert
    assertTrue(fxRateEntities.isEmpty());
}



@Test(expected = NullPointerException.class)
public void testPopulateFxRateDataWithNullInput() {
    // Arrange
    List<Result> fxRateData = null;
    String asAtDate = "2022-03-05";
    Long rateSetId = 123L;
    FxRateEntityEntityHelper helper = new FxRateEntityEntityHelper();

    // Act
    helper.populateFxRateData(fxRateData, asAtDate, rateSetId);
}
