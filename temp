    private void btn00MouseClicked(java.awt.event.MouseEvent evt) {                                   

        clicked++;
        if (btn00flag) {
            if (clicked % 2 == 0) {
                btn00.setLabel("o");
                btn00.setEnabled(false);
                btn00flag = false;
            } else {
                btn00.setLabel("X");
                btn00.setEnabled(false);
                btn00flag = false;

            }

        }
        checkWinner();

        // TODO add your handling code here:
    }                                  

